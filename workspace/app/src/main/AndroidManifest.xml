<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.him.endlessaisle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.him.view.SearchActivity"></activity>
        <activity android:name="com.him.view.PaymentActivity" />
        <activity android:name="com.him.view.OrderConfirmationActivity" />
        <activity android:name="com.him.view.AddCustomerActivity" />
        <activity android:name="com.him.view.BuddyStoreInvetoryActivity" />
        <activity android:name="com.him.view.ProductDetailActivity" />
        <activity android:name="com.him.view.LevelTwoItemListActivity" />
        <activity android:name="com.him.view.LevelOneItemListActivity" />
        <activity android:name="com.him.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>